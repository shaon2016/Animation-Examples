<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.lastblade.animationpractice">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:theme="@style/AppTheme.ExplodeTransitionActivity"
                android:name=".ExplodeTransitionItemDetails">
        </activity>
        <activity
                android:name=".ExplodeTransitionActivity"
                android:theme="@style/AppTheme.ExplodeTransitionActivity">
        </activity>
        <activity android:name=".ActivityWindowTransition">
        </activity>
        <activity android:name=".ActivityWindowTransitionTwo">
        </activity>
        <activity android:name=".ActivityWindowTransitionOne">
        </activity>
        <activity android:name=".AutoLayoutAnimationActivity">
        </activity>
        <activity android:name=".FlingAnimation">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>